{
    "name": "jonashildrestrand/ddd-helpers",
    "description": "Set of artisan commands to create bounded contexts, value objects etc.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "jonashildrestrand",
            "email": "jonas@hildrestrand.net"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.1",
        "illuminate/support": "^5.8",
        "illuminate/console": "^5.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3"
    },
    "autoload": {
        "psr-4": {
            "DDDHelpers\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DDDHelpers\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    }
}
